    "multer": "^1.4.5-lts.1",
    "openai": "^4.28.0",
    "pdf-parse": "^1.1.1",
    "winston": "^3.17.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.0",
    "@types/pdf-parse": "^1.1.1",
